<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://code.alibabatech.com/schema/dubbo
            http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="dubbo-ums-provider" />

	<dubbo:registry id="ums-service" address="${zk.url}" check="false" />

	<dubbo:protocol name="dubbo" port="${zk.dubbo-port}" dispatcher="all" threadpool="cached" threads="1500" />

	<!-- 提供服务接口 --> 
	
	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.SmsCouponHistoryService" ref="smsCouponHistoryService" loadbalance="leastactive" retries="1" executes="1500"
		timeout="30000" />

	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.SmsCouponService" ref="smsCouponService" loadbalance="leastactive" retries="1" executes="1500" timeout="30000" />
	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.SmsFlashPromotionService" ref="smsFlashPromotionService" loadbalance="leastactive" retries="1" executes="1500"
		timeout="30000" />

	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.SmsFlashPromotionSessionService" ref="smsFlashPromotionSessionService" loadbalance="leastactive" retries="1"
		executes="1500" timeout="30000" />

	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.SmsHomeAdvertiseService" ref="smsHomeAdvertiseService" loadbalance="leastactive" retries="1" executes="1500"
		timeout="30000" />
	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.UmsIntegrationConsumeSettingService" ref="umsIntegrationConsumeSettingService" loadbalance="leastactive" retries="1"
		executes="1500" timeout="30000" />

	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.UmsMemberService" ref="umsMemberService" loadbalance="leastactive" retries="1" executes="1500" timeout="30000" />
	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.UmsMemberLevelService" ref="umsMemberLevelService" loadbalance="leastactive" retries="1" executes="1500" timeout="30000" />

	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.UmsMemberReceiveAddressService" ref="umsMemberReceiveAddressService" loadbalance="leastactive" retries="1"
		executes="1500" timeout="30000" />

	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.UmsMemberCouponService" ref="umsMemberCouponService" loadbalance="leastactive" retries="1" executes="1500"
		timeout="30000" />
	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.UmsPermissionService" ref="umsPermissionService" loadbalance="leastactive" retries="1" executes="1500" timeout="30000" />

	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.UmsRoleService" ref="umsRoleService" loadbalance="leastactive" retries="1" executes="1500" timeout="30000" />
	
	<dubbo:service registry="ums-service" interface="com.zscat.ums.service.GroupService" ref="groupService" loadbalance="leastactive" retries="1" executes="1500" timeout="30000" />



</beans>