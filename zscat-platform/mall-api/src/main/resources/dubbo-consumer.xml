<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://code.alibabatech.com/schema/dubbo
            http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="mall-api" />
	<dubbo:registry id="pms-service"  address="${zk.url}" check="false"/>
	<dubbo:registry id="ums-service"  address="${zk.url}" check="false"/>

	<dubbo:reference registry="pms-service" id="pmsMerchandiseService" interface="com.zscat.pms.service.PmsMerchandiseService" check="false"/>
	<dubbo:reference registry="pms-service" id="pmsStoreMerchantService" interface="com.zscat.pms.service.PmsStoreMerchantService" check="false"/>
	<dubbo:reference registry="pms-service" id="slideShowModelService" interface="com.zscat.pms.service.SlideShowModelService" check="false"/>
	<!-- 添加用户接口 时间2019.7.24 姓名张古良 -->
	<dubbo:reference registry="ums-service" id="umsAdminService" interface="com.zscat.ums.service.UmsAdminService" check="false"/>

</beans>